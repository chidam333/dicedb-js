{
  "name": "dicedb-sdk",
  "description": "A simple, fast, and efficient dice-db client",
  "version": "1.1.0",
  "homepage": "https://dicedb.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/chidam333/dicedb-js.git"
  },
  "license": "BSD-3-Clause",
  "main": "dist/index.js",
  "module": "index.ts",
  "types": "src/index.ts",
  "devDependencies": {
    "@bufbuild/buf": "^1.51.0",
    "@bufbuild/protoc-gen-es": "^2.2.5",
    "@types/bun": "latest"
  },
  "scripts": {
    "build": "bun build index.ts --outdir=dist --minify",
    "package": "bun build src/index.ts --target=node --outdir=dist --minify",
    "dev": "bun index.ts",
    "test": "bun test",
    "lint": "bun run eslint .",
    "format": "bun run prettier . --write"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.2.5",
    "google-protobuf": "^3.21.4"
  },
  "keywords": [
    "dice-db",
    "dicedb client",
    "type-Safe database sdk",
    "real-time data watcher",
    "protobuf integration"
  ],
  "files": [
    "dist",
    "src"
  ]
}
